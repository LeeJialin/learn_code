#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/bin/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/bin/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/registry.npmmirror.com+resolve@1.22.4/node_modules:/mnt/d/LiJialin/死路一条/learn_code/learn_Vite/07_juejin_10/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../../registry.npmmirror.com+resolve@1.22.4/node_modules/resolve/bin/resolve" "$@"
fi
